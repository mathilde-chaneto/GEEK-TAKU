							</li>

							<li>
								<a href="{{ path('geek-taku_video') }}">
									Videos

								</a>
							</li>
							<li>
								<a href="{{ path('geek-taku_illustrations') }}">
									Illustrations

								</a>
							</li>
							<li>
								<a href="{{ path('geek-taku_projets') }}">
									Projets

								</a>
							</li>
							<li>
								<a href="{{ path('geek-taku_competences') }}">
									Competences

								</a>
							</li>

						</ul>
					</nav>

