							<span class="bar1"></span>
							<span class="bar2"></span>
							<span class="bar3"></span>
						</label>

                        <nav id="menu">
                        <ul>
                          <li><a href="{{ path('geek-taku_main') }}">Accueil</a></li>
                          <li><a href="{{ path('geek-taku_video') }}">Videos</a></li>
                          <li><a href="{{ path('geek-taku_illustrations') }}">Illustrations</a></li>
                          <li><a href="{{ path('geek-taku_projets') }}">Projets</a></li>
                          <li><a href="{{ path('geek-taku_competences') }}">Competences</a></li>
                        </ul>
						</nav>
					</form>
					</section>
				</div>
